'
BEGIN TRANSACTION;

--
-- Table: errors
--
CREATE TABLE errors (
  code int NOT NULL,
  id INTEGER PRIMARY KEY NOT NULL,
  message varchar NOT NULL
);

CREATE INDEX pk_id ON errors (id);

--
-- Table: pets
--
CREATE TABLE pets (
  id INTEGER PRIMARY KEY NOT NULL,
  name varchar NOT NULL,
  tag varchar
);

CREATE INDEX pk_id02 ON pets (id);

COMMIT;'
